{
    "version": 2,
    "builds": [
        {
            "src": "api/server.mjs"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)", 
            "dest": "/api/server.mjs" 
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.html"
        }
    ]
}