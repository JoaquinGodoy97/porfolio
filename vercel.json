{  
    "functions": {
    "api/server.mjs": {
        "memory": 1024,
        "maxDuration": 10
        }
    },
    "routes": [
        {
            "src": "/api/(.*)", 
            "dest": "/api/server.mjs" 
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.html"
        }
    ]
}